{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "npm run update-webdriver",
    "start": "webpack-dev-server --hot --env development",
    "start:with:nodeserv": "concurrently \"npm run start\" \"node ./server\"",
    "build": "npm run clean:build && webpack --env production",
    "build:github": "npm run clean:build && webpack --env github",
    "serv": "static build",
    "prodserv": "npm run clean:build && npm run build && npm run serv",
    "test:single": "npm run clean:coverage && karma start karma.conf.js --single-run --browsers PhantomJS",
    "test:watch": "npm run clean:coverage && karma start karma.conf.js --browsers PhantomJS",
    "update-webdriver": "webdriver-manager update",
    "test:protractor": "npm run clean:e2e && webpack --config webpack.protractor.config.js && protractor protractor.config.js",
    "protractor": "concurrently \"npm run prodserv\" \"npm run test:protractor\"",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf tmp",
    "clean:e2e": "rimraf e2e-tests/tmp",
    "clean:all": "rimraf build tmp e2e-tests/tmp",
    "codestyle": "eslint src",
    "precommit": "npm run codestyle"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.10",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.5",
    "pre-commit": "^1.2.2",
    "promise-polyfill": "^7.1.2",
    "protractor": "^5.3.2",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "eslint": "^5.0.1",
    "express": "^4.16.3"
  }
}
